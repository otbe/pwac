{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "properties": {
        "browsers": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "env": {
            "enum": [
                "react"
            ],
            "type": "string"
        },
        "images": {
            "anyOf": [
                {
                    "properties": {
                        "sizes": {
                            "items": {
                                "type": "number"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                {
                    "enum": [
                        false
                    ],
                    "type": "boolean"
                }
            ]
        },
        "manifest": {
            "anyOf": [
                {
                    "properties": {
                        "background_color": {
                            "type": "string"
                        },
                        "description": {
                            "type": "string"
                        },
                        "icons": {
                            "items": {
                                "properties": {
                                    "sizes": {
                                        "items": {
                                            "type": "number"
                                        },
                                        "type": "array"
                                    },
                                    "src": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "type": "array"
                        },
                        "name": {
                            "type": "string"
                        },
                        "short_name": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                {
                    "enum": [
                        false
                    ],
                    "type": "boolean"
                }
            ]
        },
        "prerender": {
            "anyOf": [
                {
                    "properties": {
                        "routes": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                {
                    "enum": [
                        false
                    ],
                    "type": "boolean"
                }
            ]
        }
    },
    "type": "object"
}

